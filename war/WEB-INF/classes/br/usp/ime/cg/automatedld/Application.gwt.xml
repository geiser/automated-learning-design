<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module SYSTEM "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">
<module rename-to='automatedld'>
	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits name="com.google.gwt.user.User" />
	<inherits name="com.google.gwt.http.HTTP" />
	<inherits name="com.google.gwt.xml.XML" />
	<inherits name="com.google.gwt.json.JSON" />

	<!-- Inherit the default GWT style sheet. You can change -->
	<!-- the theme of your GWT application by un commenting -->
	<!-- any one of the following lines. -->
<!--	<inherits name="com.google.gwt.user.theme.standard.Standard" />-->
	<inherits name="com.extjs.gxt.themes.Themes" />
	<inherits name="com.extjs.gxt.charts.Chart" />
	 <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> 
	<!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/> -->

	<!-- Module inherits GWT Extends -->
	<inherits name="com.extjs.gxt.ui.GXT" />

	<!-- Module Piriti -->
	<!-- <inherits name="name.pehl.totoe.Totoe" /> -->
	<!-- <inherits name="com.google.gwt.inject.Inject" /> -->
	<!-- <inherits name="name.pehl.piriti.Piriti" /> -->
	<!-- <inherits name="name.pehl.piriti.gxt.PiritiGxt" /> -->

	<!-- Module dot2canvas GWT -->
<!--    <inherits name="org.unicam.gwt.graphics.CanvasGraphics" />-->

	<!-- Specify the app entry point class. -->
	<entry-point class='br.usp.ime.cg.automatedld.client.Application' />

	<!-- Reduce time of compilation -->
	<set-property name="user.agent" value="gecko1_8" />
	<set-property name="locale" value="default" />

	<!-- Specify the paths for translatable code -->
	<source path='client' />
	<source path='shared' />

</module>
